extends layout

block content
  div.container
    table.table
      tr
        th Id
        th Date
        th Name
        th Address
      for val in orders
        tr
          td: a(href= 'http://localhost:3000/order/#{val._id}' class="list-group-item")= val._id
          td: p= val.orderDate
          td: P= val.shipName
          td: P= val.shipAddress
